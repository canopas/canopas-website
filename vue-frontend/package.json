{
  "name": "vue-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build:css": "tailwindcss -i ./src/assets/css/tailwind.css -o ./src/assets/css/output.css",
    "serve": "cross-env NODE_ENV=production node server && npm run build:css",
    "dev": "node server && npm run build:css",
    "build": "npm run build:client && npm run build:server && npm run build:css",
    "watch": "tailwindcss -i  ./src/assets/css/tailwind.css -o ./src/assets/css/output.css --watch",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && node prerender"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@ivanv/vue-collapse-transition": "^1.0.2",
    "@popperjs/core": "^2.11.2",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vue/cli-plugin-babel": "~5.0.0-rc.2",
    "@vue/cli-plugin-eslint": "~5.0.0-rc.2",
    "@vue/compiler-sfc": "^3.0.0",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.7",
    "axios": "^0.25.0",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-plugin-vue": "^8.5.0",
    "express": "^4.17.3",
    "gsap": "^3.9.1",
    "lottie-web": "^5.9.1",
    "postcss": "^8.4.14",
    "postcss-nesting": "^10.1.7",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "serve-static": "^1.14.2",
    "swiper": "^8.0.7",
    "tailwindcss": "^3.0.24",
    "vite-plugin-pages": "^0.21.4",
    "vue-gtag": "^2.0.1",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.0.14"
  },
  "dependencies": {
    "@vueuse/core": "^9.3.0",
    "pinia": "^2.0.14",
    "serialize-javascript": "^6.0.0",
    "vite": "^2.9.1",
    "vue": "^3.2.25"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
